              a(href="#").facebook
                i.fa.fa-facebook
            li
              a(href="#").twitter
                i.fa.fa-twitter
            li
              a(href="#").pinterest
                i.fa.fa-pinterest
            li
              a(href="#").linkedin
                i.fa.fa-linkedin
          .home02-wwa__signature
            p.text-signature Patterson 
            p.name.text-orange James Patterson
            p.job-title Founder & CEO
        .hom05-wwa__cover
          img(src="images/home05-wwa-01.jpg" alt="demo")
  //- blog
  .container
    .row
      .col-md-6
        .post-type-03
